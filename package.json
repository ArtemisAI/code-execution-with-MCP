{
  "name": "mcp-code-exec-harness",
  "version": "1.0.0",
  "description": "Production-ready AI agent harness using Code Execution with MCP pattern",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "clean": "rm -rf dist",
    "prepare-workspace": "mkdir -p workspace skills",
    "build-sandbox": "docker build -t sandbox-image-name -f Dockerfile.sandbox ."
  },
  "keywords": [
    "mcp",
    "ai-agent",
    "code-execution",
    "typescript",
    "dynamic-execution",
    "docker",
    "sandbox"
  ],
