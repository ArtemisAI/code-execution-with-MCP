  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "dockerode": "^3.3.1",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/dockerode": "^3.3.9",
    "@types/node": "^22.5.0",
    "tsc-watch": "^6.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
